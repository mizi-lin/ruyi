{
    "name": "ruyi",
    "version": "0.0.1",
    "description": "如意如意，随我心意",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/RevewAI/ruyi.git"
    },
    "scripts": {
        "build": "tsc --noEmit && vite build",
        "build:firefox": "tsc --noEmit && cross-env __FIREFOX__=true vite build",
        "build:watch": "cross-env __DEV__=true vite build -w --mode development",
        "build:firefox:watch": "cross-env __DEV__=true __FIREFOX__=true vite build -w --mode development",
        "build:hmr": "rollup --config utils/reload/rollup.config.mjs",
        "wss": "node utils/reload/initReloadServer.js",
        "dev": "yarn build:hmr && (run-p wss build:watch)",
        "dev:firefox": "yarn build:hmr && (run-p wss build:firefox:watch)",
        "test": "vitest",
        "commitlint": "commitlint --edit",
        "lint": "eslint src --ext .ts,.js,.tsx,.jsx",
        "lint:fix": "yarn lint --fix",
        "prettier": "prettier . --write",
        "prepare": "husky install"
    },
    "type": "module",
    "dependencies": {
        "@ant-design/icons": "5.3.6",
        "antd": "5.16.4",
        "axios": "1.6.8",
        "construct-style-sheets-polyfill": "3.1.0",
        "dayjs": "1.11.10",
        "echarts": "5.5.0",
        "jsencrypt": "3.3.2",
        "lodash-es": "4.17.21",
        "numeral": "2.0.6",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "react-error-boundary": "4.0.13",
        "react-helmet-async": "1.3.0",
        "react-router": "6.15.0",
        "react-router-dom": "6.15.0",
        "recoil": "0.7.7",
        "recoil-nexus": "0.5.0",
        "recoil-sync": "0.2.0",
        "webextension-polyfill": "0.11.0"
    },
    "devDependencies": {
        "@commitlint/cli": "19.3.0",
        "@commitlint/config-conventional": "19.2.2",
        "@rollup/plugin-sucrase": "5.0.2",
        "@rollup/plugin-typescript": "11.1.6",
        "@testing-library/react": "14.0.0",
        "@types/chrome": "0.0.267",
        "@types/node": "20.8.10",
        "@types/react": "18.2.37",
        "@types/react-dom": "18.2.18",
        "@types/ws": "8.5.8",
        "@typescript-eslint/eslint-plugin": "6.10.0",
        "@typescript-eslint/parser": "6.18.1",
        "@vitejs/plugin-react": "4.2.1",
        "chokidar": "3.6.0",
        "cross-env": "7.0.3",
        "eslint": "9.1.1",
        "eslint-config-airbnb-typescript": "18.0.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-import": "2.29.1",
        "eslint-plugin-jsx-a11y": "6.8.0",
        "eslint-plugin-prettier": "5.1.3",
        "eslint-plugin-react": "7.34.1",
        "eslint-plugin-react-hooks": "4.6.2",
        "fs-extra": "11.1.1",
        "husky": "8.0.3",
        "jsdom": "24.0.0",
        "less": "4.2.0",
        "less-loader": "12.2.0",
        "lint-staged": "15.2.2",
        "magic-string": "0.30.10",
        "npm-run-all": "4.1.5",
        "prettier": "3.2.5",
        "rollup": "4.17.2",
        "sass": "1.76.0",
        "ts-loader": "9.5.1",
        "tslib": "2.6.2",
        "typescript": "5.4.5",
        "vite": "5.2.11",
        "vitest": "1.5.3",
        "ws": "8.17.0"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "prettier --write",
            "eslint --fix"
        ]
    },
    "packageManager": "yarn@4.1.1"
}
